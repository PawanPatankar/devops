pipeline {
	agent {
		label {
				customWorkspace "/home/ubuntu/project-jenkins"
				}
			}
			
			environment {
							num1 = 50
							num2 = 65
							
			
			}
			
			
	stages {
		
		stage("dir") {
			steps {
				echo "hello world"
				echo "${num1}"
				}
			}
			
		stage("continuous-download") {
			steps {
				git branch: 'main', url: 'https://github.com/PawanPatankar/devops.git'
				sh "python3 pytest.py"
				
				}
			}
		}
	}
